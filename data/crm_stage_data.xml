<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- Institute CRM Pipeline Stages -->
        <record id="stage_new_inquiry" model="crm.stage">
            <field name="name">New Inquiry</field>
            <field name="sequence">1</field>
            <field name="fold" eval="False"/>
        </record>

        <record id="stage_contacted" model="crm.stage">
            <field name="name">Contacted</field>
            <field name="sequence">2</field>
            <field name="fold" eval="False"/>
        </record>

        <record id="stage_interested" model="crm.stage">
            <field name="name">Interested</field>
            <field name="sequence">3</field>
            <field name="fold" eval="False"/>
        </record>

        <record id="stage_counselling_scheduled" model="crm.stage">
            <field name="name">Counselling Scheduled</field>
            <field name="sequence">4</field>
            <field name="fold" eval="False"/>
        </record>

        <record id="stage_counselling_done" model="crm.stage">
            <field name="name">Counselling Done</field>
            <field name="sequence">5</field>
            <field name="fold" eval="False"/>
        </record>

        <record id="stage_admission" model="crm.stage">
            <field name="name">Admission</field>
            <field name="sequence">6</field>
            <field name="is_won" eval="True"/>
            <field name="fold" eval="False"/>
        </record>

        <record id="stage_lost" model="crm.stage">
            <field name="name">Lost</field>
            <field name="sequence">7</field>
            <field name="fold" eval="True"/>
        </record>

    </data>
</odoo>
